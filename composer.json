{
    "name": "lagdo/dbadmin-backpack-addon",
    "description": "Jaxon DbAdmin addon for Backpack",
    "license": "Apache",
    "authors": [
        {
            "name": "Thierry Feuzeu",
            "email": "thierry.feuzeu@gmail.com",
            "homepage": "https://github.com/feuzeu"
        }
    ],
    "homepage": "https://github.com/lagdo/dbadmin-backpack-addon",
    "keywords": [
        "Laravel", "Backpack", "Backpack for Laravel", "Backpack Addon", "Dbadmin"
    ],
    "require": {
        "illuminate/support": "^12.0",
        "backpack/crud": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "Lagdo\\Dbadmin\\Backpack\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lagdo\\Dbadmin\\Backpack\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lagdo\\Dbadmin\\Backpack\\DbAdminServiceProvider"
            ]
        }
    }
}
