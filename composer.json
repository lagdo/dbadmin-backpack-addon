{
    "name": "lagdo/dbadmin-backpack-addon",
    "description": "Jaxon DbAdmin addon for Backpack",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Thierry Feuzeu",
            "email": "thierry.feuzeu@gmail.com",
            "homepage": "https://github.com/feuzeu"
        }
    ],
    "homepage": "https://github.com/lagdo/dbadmin-backpack-addon",
    "keywords": [
        "Laravel", "Backpack", "Backpack for Laravel", "Backpack Addon", "Dbadmin"
    ],
    "require": {
        "illuminate/support": "^12.0",
        "backpack/crud": "^7.0",
        "infisical/php-sdk": "^0.0.1",
        "jaxon-php/jaxon-laravel": "^5.0",
        "jaxon-php/jaxon-upload": "^2.1",
        "lagdo/dbadmin-driver-mysql": "0.22.*",
        "lagdo/dbadmin-driver-pgsql": "0.22.*",
        "lagdo/dbadmin-driver-sqlite": "0.22.*",
        "lagdo/jaxon-dbadmin": "0.31.*",
        "lagdo/laravel-facades": "^1.0",
        "lagdo/ui-builder-bootstrap5": "0.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "Lagdo\\Dbadmin\\Backpack\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lagdo\\Dbadmin\\Backpack\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lagdo\\Dbadmin\\Backpack\\DbAdminServiceProvider"
            ]
        }
    }
}
